{
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "tuya-panel-style-block-card": ["../tuya-panel-style-block-card/lib"],
      "tuya-panel-style-button": ["../tuya-panel-style-button/lib"],
      "tuya-panel-style-button-card":["../tuya-panel-style-button-card/lib"],
      "tuya-panel-style-depict-card": ["../tuya-panel-style-depict-card/lib"],
      "tuya-panel-style-enum-card": ["../tuya-panel-style-enum-card/lib"],
      "tuya-panel-style-icon-card": ["../tuya-panel-style-icon-card/lib"],
      "tuya-panel-style-list-card": ["../tuya-panel-style-list-card/lib"],
      "tuya-panel-style-slider-card": ["../tuya-panel-style-slider-card/lib"],
      "tuya-panel-style-step-card": ["../tuya-panel-style-step-card/lib"],
      "tuya-panel-style-switch-card": ["../tuya-panel-style-switch-card/lib"]
    },
    "allowJs": true,
    "declaration": true,
    "jsx": "react",
    "module": "commonjs",
    "target": "es2018",
    "moduleResolution": "node",
    "noImplicitAny": false,
    "removeComments": false,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "outDir": "./lib",
    "sourceMap": true,
    "skipLibCheck": true,
    "lib": ["dom", "es5", "es6", "es2015", "es2016", "es2017", "es2018", "esnext"],
  },
  "include": ["./src/"],
  "exclude": ["./node_modules"]
}